{
    "name": "siketyan/kagawa-bundle",
    "description": "Kagawa Prefecture restriction for Symfony.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Naoki Ikeguchi",
            "email": "root@siketyan.dev"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.3",
        "geoip2/geoip2": "^2.10",
        "symfony/framework-bundle": "^4.4",
        "symfony/twig-bundle": "^4.4",
        "symfony/webpack-encore-bundle": "^1.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Siketyan\\KagawaBundle\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "tests": "vendor/bin/phpunit src/Tests"
    }
}
